{
    {{! Query params}}
    {{#if endpointConfig.params}}
    params: {
        {{#each endpointConfig.params as | param |}}
        {{#if (isEqual param.name param.value) }}
        {{{param.name}}},
        {{else}}
        {{{param.name}}}: {{param.value}},
        {{/if}}
        {{/each}}
    },
    {{/if}}
    {{! Headers}}
    {{#if endpointConfig.headers}}
    headers: {
        {{#each endpointConfig.headers as | header |}}
        '{{@key}}': '{{header}}',
        {{/each}}
    },
    {{/if}}
}