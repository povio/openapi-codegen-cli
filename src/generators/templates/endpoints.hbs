{{! Rest client import}}
{{{genImport restClientImport}}}
{{! Zod import }}
{{#if hasZodImport}}
{{{genImport zodImport}}}
{{/if}}
{{! Models import }}
{{#each modelsImports as | modelsImport |}}
{{{genImport modelsImport}}}
{{/each}}

{{#if includeNamespace}}
export namespace {{namespace}} {
{{/if}}
{{! Endpoints export }}
{{#each endpoints as | endpoint |}}
export const {{endpointName endpoint}} = ({{#if (endpointParams endpoint)}}{ {{{endpointArgs endpoint}}} }: { {{{genEndpointParams endpoint}}} }{{/if}}) => { 
    return {{../restClientName}}.{{endpoint.method}}( 
        {{! Response }}
        { resSchema: {{importedZodSchemaName endpoint.response}} }, 
        {{! Path }}
        `{{endpointPath endpoint}}`,
        {{! Body }}
        {{#with (endpointBody endpoint) as | endpointBody |}}
        {{#if endpointBody}}{{endpointBody.name}}, {{/if}}
        {{/with}}
        {{! Config }}
        {{{genEndpointConfig endpoint}}}
    )
};

{{/each}}
{{#if includeNamespace}}
}
{{/if}}