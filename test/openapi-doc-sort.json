    "/api/ticket": {
      "get": {
        "operationId": "TicketController_list",
        "summary": "List Tickets",
        "parameters": [
          {
            "name": "page",
            "required": false,
            "in": "query",
            "description": "Represents pagination page: min 1, default 1.",
            "schema": { "type": "number" }
          },
          {
            "name": "limit",
            "required": false,
            "in": "query",
            "description": "Represents pagination limit: min 1, max 100, default 20.",
            "schema": { "type": "number" }
          },
          {
            "name": "order",
            "required": false,
            "in": "query",
            "description": "Represents format and direction of ordering. Possible orders: +id,-id,+title,-title,+description,-description,+status,-status.",
            "schema": { "type": "string" }
          },
          {
            "name": "filter",
            "required": false,
            "in": "query",
            "schema": { "$ref": "#/components/schemas/TicketFilter" }
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "title": "PaginatedListResponseOfTicketResponseDTO",
                  "allOf": [
                    { "$ref": "#/components/schemas/PaginatedListDto" },
                    {
                      "properties": {
                        "items": { "type": "array", "items": { "$ref": "#/components/schemas/TicketResponseDTO" } },
                        "filter": { "type": "object", "$ref": "#/components/schemas/TicketFilter" }
                      }
                    }
                  ]
                }
              }
            }
          }
        },
        "tags": ["Ticket"]
      },
      "post": {
        "operationId": "TicketController_createTicket",
        "summary": "Create New Ticket",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": { "application/json": { "schema": { "$ref": "#/components/schemas/NewTicketDTO" } } }
        },
        "responses": {
          "200": {
            "description": "",
            "content": { "application/json": { "schema": { "$ref": "#/components/schemas/TicketResponseDTO" } } }
          }
        },
        "tags": ["Ticket"],
        "security": [{ "access-token": [] }]
      }
    },
    "/api/ticket/{id}": {
      "get": {
        "operationId": "TicketController_getById",
        "summary": "Get Ticket By Id",
        "parameters": [{ "name": "id", "required": true, "in": "path", "schema": { "type": "string" } }],
        "responses": {
          "200": {
            "description": "",
            "content": { "application/json": { "schema": { "$ref": "#/components/schemas/TicketResponseDTO" } } }
          }
        },
        "tags": ["Ticket"]
      },
      "delete": {
        "operationId": "TicketController_deleteTicket",
        "summary": "Delete Ticket",
        "parameters": [{ "name": "id", "required": true, "in": "path", "schema": { "type": "string" } }],
        "responses": { "200": { "description": "" } },
        "tags": ["Ticket"],
        "security": [{ "access-token": [] }]
      }
    },
    "/api/ticket/{id}/status": {
      "put": {
        "operationId": "TicketController_updateTicketStatus",
        "summary": "Update Ticket Status",
        "parameters": [{ "name": "id", "required": true, "in": "path", "schema": { "type": "string" } }],
        "requestBody": {
          "required": true,
