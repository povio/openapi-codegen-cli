        "security": [{ "access-token": [] }]
      }
    }
  },
  "info": { "title": "Api Documentation", "description": "", "version": "<<RELEASE>>", "contact": {} },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "access-token": {
        "scheme": "Bearer",
        "bearerFormat": "Bearer",
        "name": "Authorization",
        "type": "http",
        "in": "Header"
      }
    },
    "schemas": {
      "ApiStatusDto": {
        "type": "object",
        "properties": {
          "test": { "$ref": "#/components/schemas/UpdateTicketStatusDto" },
          "uptime": { "type": "string", "description": "Api Status Uptime", "example": "1d 12h 10m 30s" },
          "stage": { "type": "string", "description": "Api Status Stage", "example": "project-dev" },
          "version": { "type": "string", "description": "Api Status Version", "example": "project-dev-1.0.0" },
          "release": {
            "type": "string",
            "description": "Api Status Release (Git Hash Format)",
            "example": "9eabf5b536662000f79978c4d1b6e4eff5c8d785"
          },
          "buildTime": {
            "type": "string",
            "description": "Api Status Build Time",
            "example": "2022-01-01T10:00:00.000Z"
          }
        },
        "required": ["uptime", "stage", "version", "release", "buildTime"]
      },
      "AuthLoginDto": {
        "type": "object",
        "properties": {
          "email": { "type": "string", "description": "Email" },
          "password": { "type": "string", "description": "Password" }
        },
        "required": ["email", "password"]
      },
      "AuthResponseDto": {
        "type": "object",
        "properties": {
          "accessToken": { "type": "string", "description": "Access Token" },
          "refreshToken": { "type": "string", "description": "Refresh Token" }
        },
        "required": ["accessToken", "refreshToken"]
      },
      "AuthRegisterDto": {
        "type": "object",
        "properties": {
          "email": { "type": "string", "description": "Email" },
          "password": { "type": "string", "description": "Password" }
        },
        "required": ["email", "password"]
      },
      "AuthRefreshTokenDto": {
        "type": "object",
        "properties": {
          "accessToken": { "type": "string", "description": "Access Token" },
          "refreshToken": { "type": "string", "description": "Refresh Token" }
        },
        "required": ["accessToken", "refreshToken"]
      },
      "AuthPasswordForgotDto": {
        "type": "object",
        "properties": { "email": { "type": "string", "description": "Email" } },
        "required": ["email"]
      },
      "AuthPasswordResetDto": {
        "type": "object",
        "properties": {
          "code": { "type": "string", "description": "Code" },
          "newPassword": { "type": "string", "description": "New Password" }
        },
        "required": ["code", "newPassword"]
      },
      "UserMeDto": {
        "type": "object",
        "properties": {
          "id": { "type": "string", "description": "Id" },
          "email": { "type": "string", "description": "Email" }
        },
        "required": ["id", "email"]
      },
      "TicketResponseDTO": {
        "type": "object",
        "properties": {
          "id": { "type": "string", "description": "Id of the ticket" },
          "title": { "type": "string", "description": "Title of the ticket" },
          "description": { "type": "string", "description": "Description of the ticket" },
          "status": { "type": "string", "description": "Status of the ticket" },
          "createdAt": { "type": "string", "description": "Creation time of the ticket" }
        },
        "required": ["id", "title", "description", "status", "createdAt"]
      },
      "PaginatedListDto": {
        "type": "object",
        "properties": {
          "page": { "type": "number", "description": "Page" },
          "limit": { "type": "number", "description": "Limit" },
          "order": { "description": "Limit", "type": "array", "items": { "type": "string" } },
          "filter": { "type": "object", "description": "Filter" },
          "items": { "description": "Items", "type": "array", "items": { "type": "string" } },
          "totalItems": { "type": "number", "description": "Total Items" }
        },
        "required": ["page", "limit", "totalItems"]
      },
      "TicketFilter": {
        "type": "object",
        "properties": {
          "ids": { "description": "Ids", "type": "array", "items": { "type": "string" } },
          "search": { "type": "string", "description": "Search" }
        }
      },
      "NewTicketDTO": {
        "type": "object",
        "properties": {
          "title": { "type": "string", "description": "Title of the ticket" },
          "description": { "type": "string", "description": "Description of the ticket" }
        },
        "required": ["title", "description"]
      },
      "UpdateTicketStatusDto": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "description": "New status of the ticket"
          }
        },
        "required": ["status"]
      }
    }
  }
}
